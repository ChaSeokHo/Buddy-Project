<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.fivet.buddy.mapper.MemberMapper">
    <insert id="signUp">
        insert into
            member
        values
            (member_seq.nextval, #{memberId}, #{memberPw}, #{memberName}, #{memberPhone}, sysdate, 'normal')

    </insert>

    <select id="test" resultType="Integer">
        select 1 from dual
    </select>

</mapper>