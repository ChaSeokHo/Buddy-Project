<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fivet.buddy.mapper.NoticeFileMapper">

    <!--    파일 목록 출력-->
    <select id="selectFile" resultType="com.fivet.buddy.dto.NoticeFileDTO">
        select * from notice_files where notice_seq = #{noticeSeq}
    </select>
</mapper>